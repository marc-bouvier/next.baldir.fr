---
layout: "layout.html"
pagination:
    data: collections
    size: 1
    alias: tag
permalink: /tags/{{ tag }}/
---

<h1>Étiqueté "{{ tag }}"</h1>

<ol>
    {% set taglist = collections[ tag ] %}
    {% for post in taglist | reverse %}
    <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
    {% endfor %}
</ol>
